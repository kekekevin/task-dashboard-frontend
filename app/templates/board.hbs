board

<div class="page-header">
  <h1>{{model.name}}</h1>
  <p class="lead">
  {{model.description}}
    <a href="#" class="btn btn-primary pull-right">Add Task</a>
  </p>
</div>

<div class="row">
  {{#each model.swimLanes key="id" as |swimLane|}}
    <div class="col-md-3">
      <div class="panel panel-primary">
      <div class="panel-heading">{{swimLane.name}}</div>
        <ul class="list-group">
          {{#each swimLane.tasks key="id" as |task|}}
            {{lane-task summary=task.summary}}
          {{/each}}
        </ul>
      </div>
    </div>
  {{/each}}
</div>
{{outlet}}
